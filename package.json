    "@swc/core": "^1.10.9",
    "@swc/helpers": "^0.5.15",
    "@testing-library/react": "14.0.0",
    "@types/lodash": "^4.17.14",
    "@types/node": "^20.8.7",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.3",
    "eslint-webpack-plugin": "^4.2.0",
    "fork-ts-checker-webpack-plugin": "^8.0.0",
    "glob": "^10.2.7",
    "identity-obj-proxy": "3.0.0",
    "prettier": "^2.8.7",
    "replace-in-file-webpack-plugin": "^1.0.6",
